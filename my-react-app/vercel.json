{

    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
  
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "^/login(?:\\/|$)", "dest": "/index.html" },
    { "src": "^/register(?:\\/|$)", "dest": "/index.html" },
    { "src": "^/overview(?:\\/|$)", "dest": "/index.html" },
    { "src": "^/contact(?:\\/|$)", "dest": "/index.html" }
  ]
}
